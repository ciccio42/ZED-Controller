<launch>

<!-- Load camera settings on parameter server-->
<rosparam command="load" file="$(find zed_camera_controller)/config/camera_config.yaml" />

<!-- Run multicamera handler node-->
<group ns="cameras">
    <node name="camera_controller_node" pkg="zed_camera_controller" type="zed_camera_controller_node" respawn="false" output="screen"/>
</group>


</launch>